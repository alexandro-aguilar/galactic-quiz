{
  "name": "galactic-quiz",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:unit": "jest --config jest.unit.config.js",
    "cdk": "cdk",
    "dev": "rm -rf ./cdk.out && cdk synth && sam local start-api -t ./cdk.out/app-local.template.json",
    "deploy": "yarn cdklocal deploy --verbose",
    "synth": "yarn cdklocal synth --verbose",
    "bootstrap": "yarn cdklocal bootstrap --verbose"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/dotenv": "^8.2.3",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "aws-cdk": "2.1004.0",
    "aws-cdk-lib": "2.184.1",
    "aws-cdk-local": "^2.19.2",
    "constructs": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.1",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-nodejs": "^1.204.0",
    "@aws-lambda-powertools/logger": "^2.17.0",
    "@aws-lambda-powertools/metrics": "^2.17.0",
    "@aws-lambda-powertools/tracer": "^2.17.0",
    "@aws-sdk/client-bedrock-runtime": "^3.758.0",
    "@aws-sdk/client-dynamodb": "^3.767.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-dynamodb": "^3.767.0",
    "inversify": "^7.2.0",
    "reflect-metadata": "^0.2.2"
  },
  "packageManager": "yarn@4.7.0"
}
